package Ring;
import java.awt.*;
import java.awt.geom.Ellipse2D;

import javax.swing.JComponent;

@SuppressWarnings("serial")
public class Ring extends JComponent
{

	public void paintComponent(Graphics g)
	{
		Graphics2D g2 = (Graphics2D) g;
		Ellipse2D.Double FirstRing = new Ellipse2D.Double(10,10,30,30);
		Ellipse2D.Double SecondRing = new Ellipse2D.Double(25,30,30,30);
		Ellipse2D.Double ThirdRing = new Ellipse2D.Double(40,10,30,30);
		Ellipse2D.Double FourthRing = new Ellipse2D.Double(55,30,30,30);
		Ellipse2D.Double FifthRing = new Ellipse2D.Double(70,10,30,30);
		g2.setStroke(new BasicStroke(4));
		g2.draw(FirstRing);
		g2.draw(SecondRing);
		g2.draw(ThirdRing);
		g2.draw(FourthRing);
		g2.draw(FifthRing);
	}

}



