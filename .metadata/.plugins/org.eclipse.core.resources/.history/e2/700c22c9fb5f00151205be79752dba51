package ComputeTriangle;

public class Triangle {
	private int x1, x2, x3, y1, y2, y3;
	private int[] XCoordinate = {x1, x2, x3};
	private int[] YCoordinate = {y1, y2, y3};
	
	public Triangle(int x1, int y1, int x2, int y2, int x3, int y3)
	{
		this.x1 = x1;
		this.x2 = x2;
		this.x3 = x3;
		this.y1 = y1;
		this.y2 = y2;
		this.y3 = y3;
	}
	
	public void print()
	{
		System.out.println(XCoordinate.toString());
		System.out.println(YCoordinate.toString());
	}
}
